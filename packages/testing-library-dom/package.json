		"dev": "tsdown --watch"
	},
	"dependencies": {
		"@primitivestack/frontend-testing-core": "workspace:*",
		"@testing-library/user-event": "14.6.1",
		"ramda": "0.31.3"
	},
	"devDependencies": {
		"@testing-library/dom": "10.4.1",
		"@types/ramda": "0.31.0",
		"@types/react": "19.1.8"
