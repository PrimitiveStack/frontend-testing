{
	"name": "@primitivestack/frontend-testing-library-dom",
	"version": "0.0.25",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PrimitiveStack/frontend-testing.git"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
